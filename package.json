{
	"name": "onchance",
	"version": "1.3.0",
	"description": "chatbot driven web doc",
	"keywords": [
		"webdoc",
		"documentary",
		"chatbot",
		"chance",
		"randomness"
	],
	"author": "Luciano Frizzera <lucaju@gmail.com> (luciano.fluxo.art.br)",
	"contributors": [
		"Julia Salles <juliacsalles@yahoo.com.br>"
	],
	"license": "CC-BY-NC-4.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/lucaju/onchance"
	},
	"scripts": {
		"clean": "rimraf dist",
		"start": "node ./server/index.js",
		"dev": "nodemon ./server/index.js",
		"webpackServer": "webpack-dev-server --config webpack.dev.js",
		"watch": "webpack --config webpack.dev.js --watch",
		"build-dev": "npm run clean && webpack --config webpack.dev.js",
		"production": "npm run clean && webpack --mode production --config webpack.prod.js -p --progress",
		"updateVideoCollection": "node server/util/update-video-collection.js",
		"cm": "git-cz"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"dialogflow": "^1.2.0",
		"express": "^4.17.1",
		"googleapis": "^52.1.0",
		"jquery": "^3.5.1",
		"lodash": "^4.17.15",
		"material-icons": "^0.3.1",
		"materialize-css": "^1.0.0",
		"natural": "^2.1.5",
		"tiny-timer": "^1.5.0",
		"uikit": "^3.5.4",
		"uuid": "^8.2.0",
		"video.js": "^7.8.3",
		"youtube-player": "^5.5.2"
	},
	"devDependencies": {
		"clean-webpack-plugin": "^3.0.0",
		"commitizen": "^4.1.2",
		"copy-webpack-plugin": "^6.0.3",
		"css-loader": "^3.6.0",
		"eslint": "^7.3.1",
		"file-loader": "^6.0.0",
		"git-cz": "^4.7.0",
		"handlebars": "^4.7.6",
		"handlebars-loader": "^1.7.1",
		"html-webpack-plugin": "^4.3.0",
		"image-webpack-loader": "^6.0.0",
		"jsonfile": "^6.0.1",
		"mini-css-extract-plugin": "^0.9.0",
		"nodemon": "^2.0.4",
		"rimraf": "^3.0.2",
		"simple-excel-to-json": "^2.2.1",
		"terser-webpack-plugin": "^3.0.6",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^4.2.2",
		"webpackbar": "^4.0.0"
	},
	"config": {
		"commitizen": {
			"path": "git-cz"
		}
	}
}
